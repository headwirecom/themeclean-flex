<?xml version="1.0" encoding="utf-8" ?>
<jcr:root
	xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
	xmlns:jcr="http://www.jcp.org/jcr/1.0"
	xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
	jcr:primaryType="per:Component"
	jcr:title="Multilevel Navigation - Example"
>
  <jcr:content jcr:primaryType="nt:unstructured" __variations="true">
    <sample
			jcr:primaryType="nt:unstructured"
			title="Multilevel Navigation - Example"
			group="Chrome"
		>
      <jcr:content
				jcr:primaryType="nt:unstructured"
				bottompadding="0"
				toppadding="0"
				rootpage="/content/themecleanflex/pages"
				justifyitems="end"
			/>
    </sample>
    <sample-blank
			jcr:primaryType="nt:unstructured"
			title="Multilevel Navigation"
			group="Chrome"
		>
      <jcr:content jcr:primaryType="nt:unstructured" />
    </sample-blank>
  </jcr:content>
</jcr:root>
